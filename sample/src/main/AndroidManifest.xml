<?xml version="1.0" encoding="utf-8"?>
<manifest package="org.itheima.tabindicator.sample"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LineActivity"
            android:label="@string/title_activity_line">
        </activity>
        <activity
            android:name=".TriangleActivity"
            android:label="@string/title_activity_triangle">
        </activity>
        <activity
            android:name=".RectActivity"
            android:label="@string/title_activity_rect">
        </activity>
        <activity
            android:name=".SampleListActivity"
            android:label="@string/title_activity_sample_list">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>
